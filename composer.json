{
  "name": "selfphp/php-dependency-inspector",
  "description": "CLI tool for analyzing, cleaning up, and monitoring Composer dependencies in PHP projects.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SELFPHP - Damir Enseleit",
      "email": "damir.enseleit@selfphp.de"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/console": "^6.0",
    "symfony/finder": "^6.0",
    "nikic/php-parser": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "autoload": {
    "psr-4": {
      "PhpDependencyInspector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDependencyInspector\\Tests\\": "tests/"
    }
  },
  "bin": ["bin/phpdi"],
  "keywords": [
    "composer",
    "dependency",
    "inspector",
    "cleanup",
    "unused packages",
    "php",
    "cli",
    "analysis",
    "outdated packages",
    "automated audit"
  ]
}
